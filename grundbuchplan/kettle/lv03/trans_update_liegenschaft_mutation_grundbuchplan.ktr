    <replace_variables>Y</replace_variables>
    <sql>UPDATE av_grundbuch.t_liegenschaften_liegenschaft
SET mutation = TRUE
WHERE tid IN 
(
 SELECT liegen.tid
 FROM av_avdpool_ch.liegenschaften_liegenschaft as liegen, av_avdpool_ch.liegenschaften_projliegenschaft as proj
 WHERE liegen.gem_bfs = ?
 AND proj.gem_bfs = ?
 AND liegen.geometrie &amp;&amp; proj.geometrie
 AND ST_Distance(ST_PointOnSurface(liegen.geometrie), proj.geometrie) = 0
);</sql>
