INSERT INTO av_avwms.lnna ( kategorie, &quot;name&quot;, pos, ori, hali, vali, bfsnr, y, x, rot, hali_txt, vali_txt, fontsize, fontitalic, fontbold)
SELECT kategorie, &quot;name&quot;, pos, ori, hali, vali, bfsnr, y, x, rot, hali_txt, vali_txt, 14 as fontsize,
  CASE 
    WHEN kategorie = &apos;Flurname&apos; THEN 1
    ELSE 0
  END as fontitalic,
  CASE 
    WHEN kategorie = &apos;Ortsname&apos; THEN 1
    ELSE 0
  END as fontbold
FROM av_mopublic.nomenklatur__namenpos
WHERE bfsnr = ?;</sql>
