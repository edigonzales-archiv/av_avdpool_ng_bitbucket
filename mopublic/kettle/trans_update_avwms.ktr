INSERT INTO av_avwms.hadr (hausnummer, gebaeudename, pos, gwr_egid, gwr_edid, lokalisationsname, plz, zusatzziffern, ortschaftsname, ori, hali, vali, bfsnr, y, x, rot, hali_txt, vali_txt)
SELECT a.hausnummer, a.gebaeudename, b.pos, a.gwr_egid, a.gwr_edid, a.lokalisationsname, a.plz, a.zusatzziffern, a.ortschaftsname, b.ori, b.hali, b.vali, a.bfsnr, b.y, b.x, b.rot, b.hali_txt, b.vali_txt
FROM av_mopublic.gebaeudeadressen__gebaeudeeingang as a, av_mopublic.gebaeudeadressen__hausnummerpos as b
WHERE a.bfsnr = ? AND b.bfsnr = ?
AND a.tid = b.hausnummerpos_von;</sql>
